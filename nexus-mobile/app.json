{
  "expo": {
    "name": "Nexus Mobile",
    "slug": "nexus-mobile",
    "scheme": "nexus",
    "version": "1.0.0",
    "sdkVersion": "53.0.0",
    "jsEngine": "hermes",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "infoPlist": {
              "LSApplicationQueriesSchemes": [
                "plaid", "citi", "chase", "bankofamerica", "wellsfargo"
              ]
            }
          }
        }
      ]
    ],
    "platforms": [
      "ios",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "app.nexus.ai",
      "buildNumber": "1",
      "supportsTablet": true
    },
    "extra": {
      "eas": {
        "projectId": "be30e681-1247-49f1-9ea6-ee50b3eb657d"
      }
    }
  }
}

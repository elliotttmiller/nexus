{
  "expo": {
    "name": "Nexus Mobile",
    "slug": "nexus-mobile",
    "scheme": "nexus",
    "version": "1.0.1",
    "jsEngine": "hermes",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "ios": {
            "infoPlist": {
              "LSApplicationQueriesSchemes": [
                "plaid",
                "citi",
                "chase",
                "bankofamerica",
                "wellsfargo"
              ]
            }
          }
        }
      ]
    ],
    "platforms": [
      "ios",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "app.nexus.ai",
      "buildNumber": "1",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "extra": {
      "eas": {
        "projectId": "bc93e57c-3aa9-43b4-9e55-35296d034b69"
      },
      "API_BASE_URL": "https://nexus-production-2e34.up.railway.app"
    },
    "owner": "nazreid"
  }
}

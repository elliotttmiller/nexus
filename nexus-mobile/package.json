{
  "name": "nexus-mobile-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "NODE_ENV=production expo export --platform ios",
    "build:android": "NODE_ENV=production eas build --platform android",
    "build:ios": "NODE_ENV=production eas build --platform ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ai": "node test-runner.js",
    "test:ai:e2e": "node test-runner.js --e2e-only",
    "test:ai:unit": "node test-runner.js --unit-only",
    "test:ai:performance": "node test-runner.js --performance-only",
    "test:ai:health": "node test-runner.js --health-check",
    "test:ai:dev": "API_BASE_URL=http://localhost:3000/api AI_SERVICE_URL=http://localhost:8000 node test-runner.js",
    "test:ai:staging": "API_BASE_URL=https://nexus-staging.up.railway.app/api AI_SERVICE_URL=https://nexus-ai-staging.up.railway.app node test-runner.js",
    "test:ai:prod": "API_BASE_URL=https://nexus-production-2e34.up.railway.app/api AI_SERVICE_URL=https://nexus-ai-production.up.railway.app node test-runner.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "d3-shape": "^3.2.0",
    "expo": "53.0.20",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "^17.1.7",
    "expo-linking": "7.1.7",
    "expo-router": "5.1.4",
    "expo-secure-store": "14.2.3",
    "expo-splash-screen": "0.30.10",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-plaid-link-sdk": "11.13.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@testing-library/react-native": "^12.4.2",
    "@types/d3-scale": "^4.0.9",
    "@types/jest": "29.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "~19.0.10",
    "@types/react-native-svg-charts": "^5.0.16",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "axios": "^1.6.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint-plugin-react": "7.37.5",
    "jest": "~29.7.0",
    "jest-expo": "53.0.9",
    "typescript": "^5.8.3"
